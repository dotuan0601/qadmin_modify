INSERT INTO `productdetail` VALUES (1,1,'<div class=\"content\"><table class=\"table\">\r\n        <tbody>\r\n        <tr>\r\n            <td>&nbsp;Đạm tối thiểu (min %)&nbsp;</td>\r\n            <td>&nbsp;17</td>\r\n        </tr>\r\n        <tr>\r\n            <td>&nbsp;Ẩm độ (max %)<br>\r\n            </td>\r\n            <td>&nbsp;14</td>\r\n        </tr>\r\n        <tr>\r\n            <td>&nbsp;Ca (min-max %)</td>\r\n            <td>&nbsp;3,5 - 4,5</td>\r\n        </tr>\r\n        <tr>\r\n            <td>&nbsp;P (min%)</td>\r\n            <td>&nbsp;0,45 - 1</td>\r\n        </tr>\r\n        <tr>\r\n            <td>&nbsp;Xơ (max %)</td>\r\n            <td>&nbsp;7</td>\r\n        </tr>\r\n        <tr>\r\n            <td>&nbsp;Năng lượng trao đổi ME (min Kcal/kg)</td>\r\n            <td>&nbsp;2.650</td>\r\n        </tr>\r\n        <tr>\r\n            <td>&nbsp;Lysine tổng số ( min %)</td>\r\n            <td>&nbsp;0,85</td>\r\n        </tr>\r\n        <tr>\r\n            <td>&nbsp;Methionine +Cystine tổng số (min %)</td>\r\n            <td>&nbsp;0,7</td>\r\n        </tr>\r\n        <tr>\r\n            <td>&nbsp;Hormone</td>\r\n            <td>&nbsp;Không có</td>\r\n        </tr>\r\n        </tbody>\r\n    </table>\r\n</div>','Khô dầu đậu nành, bột cá, tấm gạo, cám gạo, bắp, cám mì, khoai mì, các lạo acid amin, khoáng và vitamin,…','<ul>\r\n    <li>Thức ăn hỗn hợp dành cho gà đẻ thương phẩm</li>\r\n    <li>Không cần bổ sung bất cứ loại thức ăn nào khác</li>\r\n    <li>Cung cấp đầy đủ nước uống sạch và mát</li>\r\n    <li>Bảo quản nơi khô ráo và thoáng mát</li>\r\n</ul>','2018-07-10 08:57:48','2018-07-10 08:57:48',NULL);
